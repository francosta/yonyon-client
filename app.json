{
  "expo": {
    "name": "YonYon",
    "slug": "yonyon",
    // missing sdkversion and privacy
    "platforms": ["ios", "android"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/splashLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#310B3B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.yonyon",
      "buildNumber": "0.0.1"
    },
    "android": {
      "permissions": [],
      "package": "io.yonyon",
      "versionCode": 1
    }
  }
}
